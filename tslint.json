{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
      "interface-name": false,
      "semicolon": false,
      "quotemark": false,
      "trailing-comma": false,
      "arrow-parens": false,
      "ordered-imports": false,
      "no-shadowed-variable": false,
      "max-classes-per-file": false,
      "member-access": false,
      "no-var-requires": false
    }
}
