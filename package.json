{
  "name": "vscode-prettier-react-snippets",
  "displayName": "React Snippets",
  "description": "Opinionated snippet generator",
  "version": "0.1.0",
  "publisher": "felicio",
  "repository": {
    "type": "git",
    "url": "https://github.com/prettier-snippets/vscode-prettier-react-snippets.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Snippets",
    "Formatters"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc --project ./",
    "watch": "tsc --watch --project ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^7.0.43",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "prettier": "^1.8.2"
  }
}
